<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" metadata-complete="true" version="2.5">

	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>OpenBankingController</servlet-name>
		<servlet-class>openbanking.demo.OpenBankingController</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>MetroController</servlet-name>
		<servlet-class>openbanking.demo.metro.MetroController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>OpenBankingController</servlet-name>
		<url-pattern>/OpenBankingController</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>MetroController</servlet-name>
		<url-pattern>/MetroController</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>page</servlet-name>
		<jsp-file>/login.jsp</jsp-file>
	</servlet>
	<servlet-mapping>
		<servlet-name>page</servlet-name>
		<url-pattern>/login.html</url-pattern>
	</servlet-mapping>


	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
</web-app>
